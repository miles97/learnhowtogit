}
.rating_header_right{
    width: 95%;
    overflow: hidden;
    padding-bottom: 20px;
    background-color: #fff;
    margin: 10rpx;
    border-radius: 20rpx;
}
.detail_charts{
    padding-left:60rpx;
    background-color:#fff;
    line-height:91rpx;
    margin:28rpx;
    border-radius:20rpx;
}
.detail_charts image{
    width: 45rpx;
    height: 45rpx;
    float: right;
    padding-right: 20rpx;
    padding-top:25rpx;
}
.nutrition_contain{
    margin-left: 20rpx;
}
.fix_icon{
    width: 50px;
    height: 50px;
    border-radius: 50%;
    position: fixed;
    right: 20px;
    bottom: 20px;
    text-align: center;
    vertical-align: middle;
    line-height: 50px;
    background-color: #000;
    color: #fff;
    font-size: 1.5rem;
}
.error-msg{
    margin: 20rpx;
    background: #fff;
    border-radius: 10rpx;
    line-height: 60rpx;
    height: 200rpx;
}