.main {
    padding: 0;
    margin: 0;
    height: 100%;
    width: 100%;
}

.imageD {
    width: 100%;
    height: 100%;
    display: inline-block;
    vertical-align: middle;
}

.right-block {
    width: 33%;
    float: left;
    text-align: center;
    margin-top: 20rpx;
}

.right-block-two {
    width: 49%;
    float: left;
    text-align: center;
    margin-top: 20rpx;
}

